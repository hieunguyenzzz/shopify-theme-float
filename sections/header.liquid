<noscript>{{ 'component-list-menu.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-search.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-menu-drawer.css' | asset_url | stylesheet_tag }}</noscript>
<noscript>{{ 'component-cart-notification.css' | asset_url | stylesheet_tag }}</noscript>
<style>
  /* Icon 1 */

  .nav-icon {
    width: 20px;
    height: 15px;
    position: relative;
    margin: auto;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: .5s ease-in-out;
    -moz-transition: .5s ease-in-out;
    -o-transition: .5s ease-in-out;
    transition: .5s ease-in-out;
    cursor: pointer;
  }

  .nav-icon span,
  #nav-icon3 span,
  #nav-icon4 span {
    display: block;
    position: absolute;
    height: 2px;
    width: 100%;
    background: rgb(var(--color-foreground));
    border-radius: 3px;
    opacity: 1;
    left: 0;
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition: .25s ease-in-out;
    -moz-transition: .25s ease-in-out;
    -o-transition: .25s ease-in-out;
    transition: .25s ease-in-out;
    -webkit-transition-delay: 0.2s, 0s;
    transition-delay: 0.2s, 0s;
  }

  .nav-icon span:nth-child(1) {
    top: 0px;
  }

  .nav-icon span:nth-child(2) {
    top: 6px;
  }

  .nav-icon span:nth-child(3) {
    top: 12px;
  }

  .menu-opening .nav-icon span:nth-child(1) {
    top: 6px;
    -webkit-transform: rotate(135deg);
    -moz-transform: rotate(135deg);
    -o-transform: rotate(135deg);
    transform: rotate(135deg);
  }

  .menu-opening .nav-icon span:nth-child(2) {
    opacity: 0;
    left: -20px;
  }

  .menu-opening .nav-icon span:nth-child(3) {
    top: 6px;
    -webkit-transform: rotate(-135deg);
    -moz-transform: rotate(-135deg);
    -o-transform: rotate(-135deg);
    transform: rotate(-135deg);
  }
</style>
<style>
  {
    % if section.settings.enable_sticky_header %
  }

  #shopify-section-header {
    position: sticky;
    top: 0;
  }

    {
    % endif %
  }

  header-drawer {
    justify-self: start;
  }



  .menu-drawer-container {
    display: flex;
  }

  .menu-drawer-overlay {
    display: block !important;
    content: "";
    visibility: visible;
    position: absolute;
    top: 0;
    right: 0;
    width: 100vw;
    height: 100vh;
    background-color: #fff;
    opacity: 0.7;
    visibility: hidden;
    transition: visibility 0.2s ease-in-out;
  }

  .menu-opening .menu-drawer-overlay {
    visibility: visible;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }


  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: 1.3;
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: 1.8;
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }

  body {
    transition: transform 0.2s ease-in;
  }

  .overflow-hidden-tablet {
    transform: translateX(-300px);
  }

  #menu-drawer {
    /* display: none; */
    overflow: hidden;
    border: none;
    padding: 0;
    width: 0;
  }

  .overflow-hidden-tablet #menu-drawer {
    position: fixed;
    display: block;
    width: 300px;
    top: 0;
    transform: translateY(0);
    visibility: visible;
  }

  .menu-drawer__inner-container {
    opacity: 0;
    transform: translate3d(100%, 0, 0);
    visibility: hidden;
    transition: all 0.5s ease 0.2s;
  }

  .overflow-hidden-tablet .menu-drawer .menu-drawer__inner-container {
    opacity: 1;
    visibility: visible;
    transform: translate3d(0, 0, 0);
  }
</style>

